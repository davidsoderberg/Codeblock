{
	"name": "davidsoderberg/Codeblock",
	"description": "The Codeblock application.",
	"keywords": ["codeblock", "methods", "functions", "blocks"],
	"license": "MIT",
	"type": "project",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/davidsoderberg/iseed"
      }
    ],
	"require": {
		"laravel/framework": "5.0",
        "illuminate/html": "@dev",
        "davidsoderberg/iseed": "master",
        "firebase/php-jwt": "1.*",
        "laravel/socialite": "2.*",
        "yzalis/identicon": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.4.0@stable"
	},
	"autoload": {
		"classmap": [
			"database"
		],
        "psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
            "tests/TestCase.php",
			"tests/UnitCase.php",
            "tests/FunctionalCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
