{
	"name": "davidsoderberg/Codeblock",
	"description": "The Codeblock application.",
	"keywords": [
		"codeblock",
		"methods",
		"functions",
		"blocks"
	],
	"license": "MIT",
	"type": "project",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/davidsoderberg/iseed"
		}
	],
	"require": {
		"laravel/framework": "5.2.*",
		"laravelcollective/html": "5.2.*",
		"laravelcollective/bus": "5.2.*",
		"davidsoderberg/iseed": "master",
		"firebase/php-jwt": "1.*",
		"laravel/socialite": "2.*",
		"yzalis/identicon": "*",
		"venturecraft/revisionable": "1.*",
		"erusev/parsedown": "1.6.0",
		"erusev/parsedown-extra": "0.7.0",
		"barryvdh/laravel-debugbar": "^2.1@dev",
		"irazasyed/laravel-gamp": "dev-master",
		"spatie/laravel-analytics": "dev-master",
		"khill/lavacharts": "2.5.*",
		"pusher/pusher-php-server": "^2.2",
		"predis/predis": "^1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.4.0@stable",
		"fzaninotto/faker": "~1.4",
		"sami/sami": "3.0",
		"squizlabs/php_codesniffer": "*",
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestTrait.php",
			"tests/UnitCase.php",
			"tests/IntegrationCase.php",
			"tests/ApiCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
